{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.1",
    "parameters": {
      "moduleMultipleStorageAccounts": {
        "type": "object"
      },
      "customTages": {
        "type": "object"
      }
    },
    "variables": {
      "location": "[resourceGroup().location]"
    },
    "resources": [
        {
            "copy" : {
                "name": "storageAccounts",
                "count": "[length(parameters('moduleMultipleStorageAccounts').accounts)]"
            },
            "type": "Microsoft.Storage/storageAccounts",
            "name": "[parameters('moduleMultipleStorageAccounts').accounts[copyIndex('storageAccounts')].saName]",
            "apiVersion": "2018-02-01",
            "dependsOn": [],
            "tags": "[parameters('customTages')]",
            "location": "[variables('location')]",
            "kind": "[parameters('moduleMultipleStorageAccounts').accounts[copyIndex('storageAccounts')].kind]",
            "sku": {
                "name": "[parameters('moduleMultipleStorageAccounts').accounts[copyIndex('storageAccounts')].storageAccountType]"
            },
            "properties": {
                "accessTier": "[parameters('moduleMultipleStorageAccounts').accounts[copyIndex('storageAccounts')].accesstier]"
            }
        }
        
      ], 
    "outputs": {
    }
}

  