        "type": "object"
      },
      "location": {
        "type" : "string",
        "defaultValue": "[resourceGroup().location]"
      }
    },
    "variables": {     
    },
    "resources": [
      {
        "name": "[parameters('createApplicationSecurityGroups')[copyIndex()]]",
        "copy" : {
          "name": "asgGroups",
          "count": "[length(parameters('createApplicationSecurityGroups'))]"
        },
        "type": "Microsoft.Network/applicationSecurityGroups",
        "apiVersion": "2018-08-01",
        "comments": "module to create one or more application security groups from a passed in array of data",
        "dependsOn": [],
        "tags": "[parameters('tages')]",
        "location": "[parameters('location')]",
        "properties": {}  
      }
    ],
    "outputs": {
    }
}
