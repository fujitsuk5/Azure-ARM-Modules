{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.1",
    "parameters": {
      "moduleCreateApplicationSecurityGroups": { 
        "type": "object" 
      },
      "customTages": {
        "type": "object"
      }
    },
    "variables": {   
      "locationSpecified?" : "[empty(parameters('moduleCreateApplicationSecurityGroups').location)]" 
    },
    "resources": [
      {
        "condition": "[variables('locationSpecified?')]",
        "copy" : {
          "name": "asgGroups",
          "count": "[length(parameters('moduleCreateApplicationSecurityGroups').groupNames)]"
        },
        "name": "[parameters('moduleCreateApplicationSecurityGroups').groupNames[copyIndex()].name]",
        "type": "Microsoft.Network/applicationSecurityGroups",
        "apiVersion": "2018-08-01",
        "comments": "module to create one or more application security groups from a passed in array of data",
        "dependsOn": [
        ],
        "tags": "[parameters('customTages')]",
        "location": "[parameters('moduleCreateApplicationSecurityGroups').location]",
        "properties": {}  
      },
      {
        "condition": "[not(variables('locationSpecified?'))]",
        "copy" : {
          "name": "asgGroups",
          "count": "[length(parameters('moduleCreateApplicationSecurityGroups').groupNames)]"
        },
        "name": "[parameters('moduleCreateApplicationSecurityGroups').groupNames[copyIndex()].name]",
        "type": "Microsoft.Network/applicationSecurityGroups",
        "apiVersion": "2018-08-01",
        "comments": "module to create one or more application security groups from a passed in array of data",
        "dependsOn": [
        ],
        "tags": "[parameters('customTages')]",
        "location": "[resourceGroup().location]",
        "properties": {}  
      }
    ],
    "outputs": {
    }
}