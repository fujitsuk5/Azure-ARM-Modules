{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.1",
    "parameters": {
      "moduleMultiplePublicIPAddresses": {
        "type": "object"
      },
      "customTages": {
        "type": "object"
      }
    },
    "variables": {
      "location": "[resourceGroup().location]"
    },
    "resources": [
        {
            "copy" : {
                "name": "publicIPAddresses",
                "count": "[length(parameters('moduleMultiplePublicIPAddresses').pips)]"
            },
            "type": "Microsoft.Network/publicIPAddresses",
            "name": "[concat(toLower(parameters('moduleMultiplePublicIPAddresses').pips[copyIndex('publicIPAddresses')].pipName),'-pip')]",
            "apiVersion": "2018-06-01",
            "dependsOn": [],
            "tags": "[parameters('customTages')]",
            "location": "[variables('location')]",
            "properties": {
                "publicIPAllocationMethod": "[parameters('moduleMultiplePublicIPAddresses').pips[copyIndex('publicIPAddresses')].publicIPAllocationMethod]",
                "dnsSettings": {
                  "domainNameLabel": "[parameters('moduleMultiplePublicIPAddresses').pips[copyIndex('publicIPAddresses')].accesstier]"
                }
            }
           
        }
      ], 
    "outputs": {
    }
}

  